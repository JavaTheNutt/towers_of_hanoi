                <Label text="Label"/>
            </children>
            <padding>
                <Insets left="10.0"/>
            </padding>
        </StackPane>
    </top>
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
